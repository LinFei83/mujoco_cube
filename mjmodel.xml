<mujoco model="Cube 3x3x3">
  <compiler angle="radian" meshdir="assets/" texturedir="assets/"/>

  <option timestep="0.01" integrator="implicitfast"/>

  <size memory="600K"/>

  <visual>
    <global azimuth="180" elevation="-20"/>
    <headlight ambient="0.3 0.3 0.3" diffuse="0.6 0.6 0.6" specular="0 0 0"/>
  </visual>

  <statistic meansize="0.0087" extent="0.1"/>

  <default>
    <general ctrlrange="-0.05 0.05"/>
    <default class="cubelet">
      <joint type="ball" armature="0.0001" damping="0.0005" frictionloss="0.001"/>
      <geom type="mesh" condim="1" mesh="cubelet"/>
    </default>
    <default class="core">
      <geom size="0.01 0 0" contype="0" conaffinity="0" group="4"/>
    </default>
  </default>

  <asset>
    <texture type="skybox" colorspace="auto" builtin="gradient" width="512" height="3072"/>
